# Frontend API URL (WhatsApp client connects to this)
FRONTEND_URL=http://localhost:3000

# WhatsApp session configuration
WHATSAPP_SESSION_PATH=./.wwebjs_auth
WHATSAPP_SESSION_EXPIRY_MINUTES=30

# Phone number whitelist (E.164 format, comma-separated)
# Leave empty to allow all numbers
WHATSAPP_WHITELIST=
# Example: WHATSAPP_WHITELIST=+60123456789,+60198765432
